        <div id="cdpFeedbackSupervisor">
            <div class="col-md-6">
                <h3>CDP Feedback Supervisor</h3>
                <hr>

                <p>
                    {% if (userType == 'user') %}
                        {% if (formAction == 'view') %}
                            {% if ((formDataCdp.cdpStatus.id == '5') or (formDataCdp.cdpStatus.id == '6') or (formDataCdp.cdpStatus.id == '8')) %}
                                {{ formDataCdp.cdpFeedbackSupervisor }}
                            {% else %}
                                <script type="text/javascript">
                                    document.getElementById('cdpFeedbackSupervisor').style.display = 'none';
                                </script>
                            {% endif %}
                        {% elseif (formAction == 'edit') %}
                            {% if (((formDataCdp.cdpStatus.id == '2') or (formDataCdp.cdpStatus.id == '4') or (formDataCdp.cdpStatus.id == '7') or (formDataCdp.cdpStatus.id == '8')) and (formDataCdp.cdpFeedbackSupervisor != '')) %}
