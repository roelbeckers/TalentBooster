    <!-- PERSONALIA -->
    <div id="personalia" class="row">
        <div class="col-md-12">
        <h3>Personalia</h3>
        <hr>

            <table class="table">
                <tr>
                    <th width="150" style="border: none; background-color: #EEEEEE;">Name:</th>
                    <td width="250" style="border: none;">{{ formUser.fullname }}</td>
                    <th width="150" style="border: none; background-color: #EEEEEE;">Team:</th>
                    <td width="250" style="border: none;">{{ formUser.team }}</td>
                </tr>
                <tr>
                    <th style="border: none; background-color: #EEEEEE;">Supervisor:</th>
                    <td style="border: none;">{{ formUser.supervisor.fullname }}</td>
                    <th style="border: none; background-color: #EEEEEE;">Job Title:</th>
                    <td style="border: none;">{{ formUser.jobtitle }}</td>
                </tr>
                <tr>
                    <th style="border: none; background-color: #EEEEEE;">Date discussion:</th>
                    <td style="border: none;">
                        {% if (formProgress == 'cdp') %}
                            {% if ((userType == 'user') and ((formAction == 'create') or (formAction == 'edit'))) %}
                                {{ form_widget(formForm.cdpFeedbackDate) }}
                            {% else %}
                                {{ formDataCdp.cdpFeedbackDate|date('d-M-Y') }}
                            {% endif %}
                        {% elseif (formProgress == 'my') %}
                            {{ formDataMy.myFeedbackDate|date('d-M-Y') }}
                        {% elseif (formProgress == 'ye') %}
                            {% if ((userType == 'user') and ((formAction == 'create') or (formAction == 'edit'))) %}
                                {{ form_widget(formForm.yeFeedbackDate) }}
                            {% else %}
                                {{ formDataYe.yeFeedbackDate|date('d-M-Y') }}
                            {% endif %}
                        {% endif %}
                    </td>
                    <th style="border: none; background-color: #EEEEEE;">Status:</th>
                    <td style="border: none;">
                        {% if ((formProgress == 'cdp') and (formAction != "create")) %}
                            {{ formDataCdp.cdpStatus }}
                        {% elseif (formProgress == 'my') %}
                            {{ formDataMy.myStatus }}
                        {% elseif ((formProgress == 'ye') and (formAction != "create")) %}
                            {{ formDataYe.yeStatus }}
                        {% else %}
                            Available
                        {% endif %}
                    </td>
                </tr>
            </table>
        </div>
    </div>

    <!-- CDP FEEDBACK -->
    <div id="cdpFeedback" class="row" style="padding-top: 40px;">
